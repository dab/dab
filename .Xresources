!SETTINGS
Xft.dpi: 227
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!URXVT SETTINGS
!URxvt*font: -jmk-Neep-Medium-R-Normal--20-180-75-75-C-100-ISO8859-1
!URxvt*font: -*-terminus-*-*-*-*-24-*-*-*-*-*-*-u
            -wuncon-siji-medium-r-normal--10-100-75-75-c-80-iso10646-1
!URxvt*font: -misc-fixed-*-*-normal-*-20-200-75-75-*-100-*-r, \
            -wuncon-siji-medium-r-normal--10-100-75-75-c-80-iso10646-1
URxvt*font: xft:InputMonoCompressed Nerd Font:size=7
URxvt*internalBorder:  20
URxvt*scrollBar:       false
URxvt.geometry:        92x23
URxvt*saveLines:       5000
URxvt*cursorUnderline: false
URxvt*cursorBlink:     true
URxvt*allow_bold:      true
URxvt*depth: 24

!COLORS
*highlightColor: #2B2B29
*cursorColor: 	 #AFB0AB
*borderColor: 	 #21211F
*background:  	 #21211F
*foreground:  	 #B8B9B4

!BLACK
*color0:  #262624
*color8:  #2B2B29
!RED
*color1:  #939490
*color9:  #989995
!GREEN
*color2:  #5E5E5C
*color10: #636361
!YELLOW
*color3:  #B6B8B3
*color11: #BBBDB8
!BLUE
*color4:  #727370
*color12: #777875
!MAGENTA
*color5:  #D1D1CB
*color13: #D6D6D0
!CYAN
*color6:  #636361
*color14: #696966
!WHITE
*color7:  #AAABA6
*color15: #AFB0AB

!PERL
URxvt*perl-lib:        /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher

!CLICKABLE LINKS
URxvt*url-launcher:   google-chrome-beta
URxvt*matcher.button: 1

!2BWM COLORS
!twobwm.border_width: 	   8
!twobwm.outer_border: 	   8
!twobwm.focus_color: 	   #A8A8A6
!twobwm.unfocus_color: 	   #353533
